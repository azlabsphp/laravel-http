{
    "name": "drewlabs/laravel-http",
    "description": "Simple HTTP packages definitions",
    "type": "library",
    "license": "MIT",
    "authors": [{
        "name": "azandrew-sidoine",
        "email": "asmyns.platonnas29@gmail.com"
    }],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.2",
        "illuminate/http": "^7.0|^8.0|^9.0",
        "illuminate/support": "^7.0|^8.0|^9.0",
        "illuminate/contracts": "^7.0|^8.0|^9.0",
        "drewlabs/core-helpers": "^2.3",
        "drewlabs/contracts": "^2.5"
    },
    "autoload": {
        "psr-4": {
            "Drewlabs\\Packages\\Http\\": "src/"
        },
        "files": [
            "helpers.php"
        ]
    },
    "repositories": [{
            "type": "vcs",
            "url": "git@github.com:liksoft/drewlabs-php-contracts.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:liksoft/drewlabs-php-core-helpers.git"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Drewlabs\\Packages\\Http\\ServiceProvider"
            ],
            "aliases": {}
        }
    },
    "suggest": {
        "nyholm/psr7": "Required to create psr7 requests using drewlabs_create_psr7_request() global function",
        "nyholm/psr7-server": "Required to create psr7 requests using drewlabs_create_psr7_request() global function",
        "symfony/psr-http-message-bridge": "Symfony Psr7 request bridge for transforming symfony request into psr7 ^2.1",
        "drewlabs/core": "Required to use validator and view model traits (^2.0)"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "nyholm/psr7": "^1.5",
        "nyholm/psr7-server": "^1.0"
    }
}
