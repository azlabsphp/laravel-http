{
    "name": "drewlabs/laravel-http",
    "description": "Provides bridge to laravel framework HTTP components",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "azandrew-sidoine",
            "email": "asmyns.platonnas29@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.2",
        "illuminate/http": "^7.0|^8.0|^9.0",
        "psr/http-message": "^1.0|^2.0",
        "drewlabs/http-contracts": "^0.2",
        "drewlabs/core-helpers": "^0.2",
        "drewlabs/contracts": "^0.2",
        "drewlabs/cors": "^0.2.0",
        "drewlabs/overloadable": "^0.1.10"
    },
    "autoload": {
        "psr-4": {
            "Drewlabs\\Packages\\Http\\": "src/"
        },
        "files": [
            "helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Drewlabs\\Packages\\Http\\ServiceProvider"
            ]
        }
    },
    "suggest": {
        "nyholm/psr7": "Required to create psr7 requests using drewlabs_create_psr7_request() global function",
        "nyholm/psr7-server": "Required to create psr7 requests using drewlabs_create_psr7_request() global function",
        "symfony/psr-http-message-bridge": "Symfony Psr7 request bridge for transforming symfony request into psr7 ^2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "nyholm/psr7": "^1.5",
        "nyholm/psr7-server": "^1.0"
    }
}
