{
    "name": "drewlabs/http",
    "description": "Simple HTTP packages definitions",
    "type": "library",
    "license": "MIT",
    "authors": [{
        "name": "azandrew-sidoine",
        "email": "asmyns.platonnas29@gmail.com"
    }],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.3",
        "illuminate/routing": "^8.0",
        "illuminate/support": "^8.0",
        "illuminate/contracts": "^8.0",
        "drewlabs/core": "dev-master",
        "drewlabs/helpers": "v1.8.x-dev"
    },
    "autoload": {
        "psr-4": {
            "Drewlabs\\Packages\\Http\\": "src/"
        },
        "files": [
            "helpers.php"
        ]
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:azandrew-sidoine/drewlabs-php-contracts.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:azandrew-sidoine/drewlabs-php-core.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:azandrew-sidoine/drewlabs-php-utils.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:azandrew-sidoine/drewlabs-helpers.git"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Drewlabs\\Packages\\Http\\HttpServiceProvider"
            ],
            "aliases": {}
        }
    }
}
