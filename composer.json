{
    "name": "drewlabs/http",
    "description": "Simple HTTP packages definitions",
    "type": "library",
    "license": "MIT",
    "authors": [{
        "name": "azandrew-sidoine",
        "email": "asmyns.platonnas29@gmail.com"
    }],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.2",
        "illuminate/routing": "^5.0|^6.0|^7.0|^8.0",
        "illuminate/support": "^5.0|^6.0|^7.0|^8.0",
        "illuminate/contracts": "^5.0|^6.0|^7.0|^8.0",
        "drewlabs/core": "^v1.0.x-dev",
        "drewlabs/core-helpers": "^v1.0.x-dev",
        "drewlabs/contracts": "^v1.0.x-dev",
        "symfony/psr-http-message-bridge": ">=2.1",
        "nyholm/psr7-server": ">=1.0"
    },
    "autoload": {
        "psr-4": {
            "Drewlabs\\Packages\\Http\\": "src/"
        },
        "files": [
            "helpers.php"
        ]
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:liksoft/drewlabs-php-contracts.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:liksoft/drewlabs-php-core.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:liksoft/drewlabs-php-core-helpers.git"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Drewlabs\\Packages\\Http\\ServiceProvider"
            ],
            "aliases": {}
        }
    }
}
